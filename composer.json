{
    "name": "nightfallstudios/social-media-manager",
    "description": "Manage user's social media accounts. Post and get statuses and see stats",
    "type": "craft-plugin",
    "version": "1.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "social media manager"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Nightfall Studios",
            "homepage": "https://www.nightfallstudios.com.au/"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0",
      "ext-curl": "*",
